{
    "name": "batopa/chirp",
    "description": "Cache for Twitter using MongoDB",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/batopa/chirp",
    "keywords": [
        "Twitter",
        "MongoDB",
        "cache",
        "API"
    ],
    "authors": [
        {
            "name": "Alberto Pagliarini",
            "homepage": "https://github.com/batopa"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-mongodb": "*",
        "mongodb/mongodb": "^1.0",
        "abraham/twitteroauth": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "^2.5.1"
    },
    "autoload": {
        "psr-4": {
            "Bato\\Chirp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr4": {
            "Bato\\Chirp\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
